country_code={{ dwst_iface_country_code }}
interface={{ dwst_iface_name }}
ssid={{ dwst_ssid }}
{% set hw_mode_value = dwst_hwmode if (dwst_hwmode != "n") else "g" %}
hw_mode={{ hw_mode_value }}
driver={{ dwst_driver }}
{% if dwst_hwmode=="n" %}
ieee80211n=1
{% endif %}
channel={{ dwst_channel }}
{% if dwst_40MHzbw %}
ht_capab=[SHORT-GI-40][HT40+][HT40-][DSSS_CCK-40]
{% endif %}
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0

wpa=2
wpa_passphrase={{ dwst_wpa_pass }}
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP CCMP
rsn_pairwise=CCMP
